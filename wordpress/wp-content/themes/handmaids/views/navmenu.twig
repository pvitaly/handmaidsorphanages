<ul id="navmenu" class="horizontalList">
{% for page in page_list %}
    <li {% if page.is_current_page == true %} class="current" {% endif %}>
        <a href="{{page.get_permalink}}">{{page.title}}</a>

        
    {% if page.children|e %}
        <ul class="submenu">
            {% for child in page.children %}
            <li><a href="{{child.get_permalink}}">{{child.title}}</a></li>
            {% endfor %}
        </ul>
    {% endif %}
	
	<div class="uparrow"></div> 
    
	</li>
{% endfor %}
</ul>